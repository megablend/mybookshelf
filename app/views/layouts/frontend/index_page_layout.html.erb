<!DOCTYPE html>
<html>
<head>
  <title><%= page_title(yield(:title)) %></title>
  <!-- Favicon -->
  <%= favicon_link_tag 'favicon/favicon.ico' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/frontend/shim' %>
</head>
<body class="home">

<!-- Page Container -->
<div class="wide_layout">
   <!-- Page Header -->
   <%= render 'layouts/frontend/header' %>
   <!-- End Page Header -->

   <!-- Page Home Slider -->
   <%= render 'layouts/frontend/home_slider' %>
   <!-- End Page Home Slider -->

   <!-- Services -->
   <%= render 'layouts/frontend/services' %>
   <!-- End Services -->

   <!-- Page Top -->
  <%= render 'layouts/frontend/page_top' %>
   <!-- End Page Top -->
  
  <!-- Page Content -->
  <%= yield %>
  <!-- End Page Content -->

<!-- Page Footer -->
 <%= render 'layouts/frontend/footer' %>
 <!-- End Page Footer -->

<!-- Include Libs and Plugins -->
<%= javascript_include_tag 'frontend_index_page_manifest_footer', 'data-turbolinks-track' => true %>
</body>
</html>
